<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <!-- Define the output path where the package will be stored -->
	  <!--<NupkgOutputPath>$(Env:LocalPackagePath)</NupkgOutputPath>-->
